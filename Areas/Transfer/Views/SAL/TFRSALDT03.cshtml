                errorAlert.init();
            }
        });
    }
    function SelectWnngF() {
        var doc = $("#SALE_DOC_ID").val();
        $.ajax({
            type: "POST",
            url: pathServer + "/SAL/TFRSALDT03_SELECT_WNNG_F",
            dataType: "json",
            cache: false,
            data: { SALE_DOC_ID: doc },
            success: function (data) {
                var AVY_SALES_FOTR_WNNG = data.AVY_SALES_FOTR_WNNG;
                $("#PD_DESC").val(AVY_SALES_FOTR_WNNG.PD_DESC);
                $("#PD_TOT_QTY").val(AVY_SALES_FOTR_WNNG.PD_TOT_QTY);
                $("#PD_PER_UNIT").val(AVY_SALES_FOTR_WNNG.PD_PER_UNIT);
                $("#VCNE_DESC").val(AVY_SALES_FOTR_WNNG.VCNE_DESC);
                $("#VCNE_PER_UNIT").val(AVY_SALES_FOTR_WNNG.VCNE_PER_UNIT);
                $("#LGST_DESC").val(AVY_SALES_FOTR_WNNG.LGST_DESC);
                $("#LGST_PER_UNIT").val(AVY_SALES_FOTR_WNNG.LGST_PER_UNIT);
                $("#OTHR_PLS_DESC").val(AVY_SALES_FOTR_WNNG.OTHR_PLS_DESC);
                $("#OTHR_PLS_PER_UNIT").val(AVY_SALES_FOTR_WNNG.OTHR_PLS_PER_UNIT);
                $("#OTHR_MNS_1_DESC").val(AVY_SALES_FOTR_WNNG.OTHR_MNS_1_DESC);
                $("#OTHR_MNS_1_PER_UNIT").val(AVY_SALES_FOTR_WNNG.OTHR_MNS_1_PER_UNIT);
                $("#OTHR_MNS_2_DESC").val(AVY_SALES_FOTR_WNNG.OTHR_MNS_2_DESC);
                $("#OTHR_MNS_2_PER_UNIT").val(AVY_SALES_FOTR_WNNG.OTHR_MNS_2_PER_UNIT);
                $("#AV_WGHT").val(AVY_SALES_FOTR_WNNG.AV_WGHT);
                $("#NET_WGHT").val(AVY_SALES_FOTR_WNNG.NET_WGHT);
                $("#TOT_AMT").val(AVY_SALES_FOTR_WNNG.TOT_AMT);
                $("#DIS_AMT").val(AVY_SALES_FOTR_WNNG.DIS_AMT);
                $("#NET_AMT").val(AVY_SALES_FOTR_WNNG.NET_AMT);
                CalculateWnngFoot();
            },
            error: function () {
                errorAlert.init();
            }
