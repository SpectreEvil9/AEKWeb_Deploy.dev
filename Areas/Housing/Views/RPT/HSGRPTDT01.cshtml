<script>
    $(document).ready(function () {
        if ('@ViewBag.searchSiteId' != '') {
            $("#searchSiteId").val(JSON.parse('@Html.Raw(Json.Encode(ViewBag.searchSiteId))')).trigger("change");
        }
        if ('@ViewBag.searchHousingId' != '') {
            $("#searchHousingId").val(JSON.parse('@Html.Raw(Json.Encode(ViewBag.searchHousingId))')).trigger("change");
        }

        $('#searchHousingId, #searchSiteId').select2({
            theme: "bootstrap-5",
            width: $(this).data('width') ? $(this).data('width') : $(this).hasClass('w-100') ? '100%' : 'style',
            placeholder: $(this).data('placeholder'),
            closeOnSelect: true,
            tags: true
        });

        var date = new Date();
        let firstDay = new Date(date.getFullYear(), date.getMonth() - 1, 1);
        let lastDAy = new Date(date.getFullYear(), date.getMonth() + 1, 0);

        $("#searchDateFrom").datepicker({
            todayHighlight: true,
            orientation: 'bottom left',
            format: 'dd-M-yyyy',
            todayBtn: 'linked',
            autoclose: 'true',
            assumeNearbyYear: true
        }).datepicker('setDate', firstDay);

        $("#searchDateTo").datepicker({
            todayHighlight: true,
            orientation: 'bottom left',
            format: 'dd-M-yyyy',
            todayBtn: 'linked',
            autoclose: 'true',
            assumeNearbyYear: true
        }).datepicker('setDate', 'toDay');

        hidedefault();
        $("#typeSelect").change(function () {
            $("#saveToExcel").prop('disabled',true);
            hideaffterselect();

            let typeCode = 'RB';
            if ($("#typeSelect").val() == 'RPL') {
                typeCode = 'RB';
                $("#tab1").show();
            } else if ($("#typeSelect").val() == 'BRM') {
                $("#tab2").show();
            } else if ($("#typeSelect").val() == 'MAT') {
                typeCode = 'A';
                $("#tab3").show();
            } else if ($("#typeSelect").val() == 'FRR') {
                typeCode = 'B';
                $("#tab4").show();
            } else if ($("#typeSelect").val() == 'BRN') {
                $("#tab5").show();
            } else if ($("#typeSelect").val() == 'NRS') {
                typeCode = 'C';
                $("#tab6").show();
            } else if ($("#typeSelect").val() == 'PGG') {
                $("#tab7").show();
            } else {
                hidedefault();
            }
            callAPIByHousing(typeCode);
        });
    });

    function hidedefault() {
        $("#tab1").show();
        $("#tab2").hide();
        $("#tab3").hide();
        $("#tab4").hide();
        $("#tab5").hide();
        $("#tab6").hide();
        $("#tab7").hide();
    }
    function hideaffterselect() {
        $("#tab1").hide();
        $("#tab2").hide();
        $("#tab3").hide();
        $("#tab4").hide();
        $("#tab5").hide();
        $("#tab6").hide();
        $("#tab7").hide();
    }

    function callAPIByHousing(HSGTypeCode) {
        $.ajax({
            type: "GET",
            url: pathServer + "/RPT/GET_HOUSING",
            data: {
                HSGTypeCode: HSGTypeCode,
            },
            success: function (response) {
                $('#searchHousingId').empty()
                $("#searchHousingId").append('<option value=ALL selected>ALL</option>');
                $.each(response, function (index, value) {
                    $('#searchHousingId').append($('<option/>', {
                        value: response[index].HSG_ID,
                        text: response[index].HSG_CODE
                    }));
                });
                //$('#housingId :nth-child(1)').prop('selected', true);
                //$("#SEARCH").click();
            },
            error: function (xhr) {
                //Do Something to handle error
            }
        });
    }

    $("#SEARCH").on('click', function () {
        $("#saveToExcel").prop('disabled', false);
        let typeSelect = $("#typeSelect").val();
        if (typeSelect == 'BRM') {
            tableDetailBRM();
        } else if (typeSelect == 'MAT') {
            tableDetailMAT();
