        }
    });

    $("#saveToExcel").click(function () {
        let searchSiteId = $("#searchSiteId").val();
        let searchHousingId = $("#searchHousingId").val();
        let searchDateFrom = $("#searchDateFrom").val();
        let searchDateTo = $("#searchDateTo").val();

        let api = "";
        let typeSelect = $("#typeSelect").val();
        if (typeSelect == 'RPL') {
            api = "RPLRPT01";
        } else if (typeSelect == 'BRM') {
            api = "BRMRPT01";
        } else if (typeSelect == 'MAT') {
            api = "MATRPT01";
        } else if (typeSelect == 'FRR') {
            api = "FRRRPT01";
        } else if (typeSelect == 'BRN') {
            api = "BRNRPT01";
        } else if (typeSelect == 'NRS') {
            api = "NRSRPT01";
        } else if (typeSelect == 'PGG') {
            api = "PGGRPT01";
        }
        var url = pathServer + "/RPT/" + api + "?id=Excel";
        url += "&searchSiteId=" + searchSiteId;
        url += "&searchHousingId=" + searchHousingId;
        url += "&searchDateFrom=" + searchDateFrom;
