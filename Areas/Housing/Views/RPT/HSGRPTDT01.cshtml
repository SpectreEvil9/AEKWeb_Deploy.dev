
@{
    ViewBag.Title = "HSGRPTDT01";
}

<div class="container">
    <div class="page-inner">
        <div class="page-header">
            <h3 class="fw-bold mb-3">สรุปรายงานประจำวัน</h3>
            <ul class="breadcrumbs mb-3">
                <li class="nav-home">
                    <a href="#">
                        <i class="icon-home"></i>
                    </a>
                </li>
                <li class="separator">
                    <i class="icon-arrow-right"></i>
                </li>
                <li class="nav-item">
                    <a href="#">Housing</a>
                </li>
                <li class="separator">
                    <i class="icon-arrow-right"></i>
                </li>
                <li class="nav-item">
                    <a href="#">Report</a>
                </li>
            </ul>
        </div>
        <div class="row">
            <div class="col-md-12">
                <form class="m-form m-form--fit m-form--label-align-right">
                    <div class="card">
                        <div class="card-header">
                            <h4 class="card-title">ข้อมูลภาพรวมรายงานประจำวัน</h4>
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <div class="col-md-4 col-lg-3">
                                    <div class="form-group">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text" id="basic-addon1">ฟาร์ม</span>
                                            <select class="form-select form-control" id="searchSiteId" name="searchSiteId" required disabled>
                                                <option value="ALL" selected="selected">ALL</option>
                                                @foreach (var sit in ViewBag.MAS_SITE)
                                                {
                                                    <option value="@sit.SITE_ID" @(sit.SITE_ID == "BT" ? "selected" : "")>@sit.SITE_NM_TH</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4 col-lg-3">
                                    <div class="form-group">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text col-lg-4" id="basic-addon1">ประเภทโรงเรือน</span>
                                            <select class="form-select form-control" id="typeSelect">
                                                <option value="RPL">ทดแทน</option>
                                                <option value="BRM">พ่อพันธุ์</option>
                                                <option value="MAT">ผสม-อุ้มท้อง</option>
                                                <option value="FRR">คลอด</option>
                                                <option value="BRN">รุ่นพันธุ์</option>
                                                <option value="NRS">อนุบาล</option>
                                                <option value="PGG">ขุน</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4 col-lg-2">
                                    <div class="form-group">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text" id="basic-addon1">เล้า</span>
                                            <select class="form-select form-control" id="searchHousingId" name="searchHousingId">
                                                <option value="ALL" selected="selected">ALL</option>
                                                @foreach (var hsg in ViewBag.MAS_HSG)
                                                {
                                                    <option value="@hsg.HSG_CODE">@hsg.HSG_CODE</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4 col-lg-2">
                                    <div class="form-group">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text" id="basic-addon1">เริ่มวันที่</span>
                                            <input type="text"
                                                   id="searchDateFrom"
                                                   name="searchDateFrom"
                                                   class="form-control"
                                                   placeholder="Select Date" />
                                            <span class="input-group-icon">
                                                <i class="fas fa-calendar-alt"></i>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4 col-lg-2">
                                    <div class="form-group">
                                        <div class="input-group mb-3">
                                            <span class="input-group-text" id="basic-addon1">ถึงวันที่</span>
                                            <input type="text"
                                                   id="searchDateTo"
                                                   name="searchDateTo"
                                                   class="form-control"
                                                   placeholder="Select Date" />
                                            <span class="input-group-icon">
                                                <i class="fas fa-calendar-alt"></i>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card-footer d-flex" style="justify-content: flex-end !important;">
                                <button type="button" id="SEARCH" class="btn btn-primary btn-round">Search</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
