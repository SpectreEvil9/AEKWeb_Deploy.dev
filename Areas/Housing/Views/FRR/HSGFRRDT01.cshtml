                var net = (tableMain.data()[i].BALANCE_QTY + tableMain.data()[i].FROM_HSG_MATE_QTY + tableMain.data()[i].RCV_QTY + FRRW_TOT) - (tableMain.data()[i].TFR_QTY + tableMain.data()[i].DIED_TOT + tableMain.data()[i].DSCRD_TOT);
